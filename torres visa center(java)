import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        int n;
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
        int a[]=new int[n];
        int k=0;
    while(n>0){
        int x=n%10;
      if(x%2==0){
          x=x+1;
      }
  else{
x=x-1;
  }
 a[k++]=x;
       n=n/10;
    }
for(int i=k-1;i>=0;){
    
    
System.out.printf("%d",a[i--]);
}
        
       
    }
}
      
