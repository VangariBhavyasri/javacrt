import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
         int n;
   n=s.nextInt();
    
     if(n%10>=5)
     {
         n=n+(10-n%10);
         System.out.println(n);
     }
    else
    {
     n=(n/10)*10;
        System.out.println(n);
    }
    }
}
