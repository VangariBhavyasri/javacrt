import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int n;
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
        if(n>=1&&n<=10000000){
            
            int a=1,b=1;
            int i=3,r=0;
            while(i<=n){
                r=a+b;
                a=b;
                b=r;
                i++;
            }
        System.out.printf("%d",b);
    }
}
}
      
