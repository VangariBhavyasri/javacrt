import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        int n,p=1;
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
    
        if(n>=1&&n<=10000000){

    while(n>=1){
       int x=n%10;
       if(x==0){
          }else{
        p=p*x;
        }
        n=n/10;
    }
   System.out.printf("%d",p);
    }
    }
    }
       
